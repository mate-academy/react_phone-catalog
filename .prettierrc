{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "arrowParens": "avoid",
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "jsxSingleQuote": false,
  "printWidth": 80,
  "semi": true,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "importOrder": [
    "react",
    "^@reduxjs/(.*)$",
    "i18n",
    "<THIRD_PARTY_MODULES>|classnames|framer-motion",
    "^@store/(.*)$",
    "^@context/(.*)$",
    "^@locales/(.*)$",
    "^@pages/",
    "^@routes/",
    "^@components/(.*)$",
    "^@ui/index",
    "^@hooks/(.*)$",
    "^@utils/(.*)$",
    "^[./]",
    "^[.*\\.module\\.scss]$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
