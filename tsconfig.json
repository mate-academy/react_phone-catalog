{
  "extends": "@mate-academy/students-ts-config",
  "include": [
    "src"
  ],
  "compilerOptions": {
    "sourceMap": false,
    "types": ["node", "cypress"],
    "baseUrl": "src",
    "paths": {
      "@pages/*": ["modules/pages/*"],
      "@components/*": ["modules/components/*"],
      "@context/*": ["Context/*"],
      "@img/*": ["img/*"],
      "@styles/*": ["styles/*"],
      "@models/*": ["types/*"],
      "@utils/*": ["types/*"],
      "@hooks/*": ["types/*"],
      "@api/*": ["api/*"]
    }
  }
}
